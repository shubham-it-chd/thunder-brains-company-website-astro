      data-element_type="section"
      data-settings='{"background_background":"classic"}'
    >
      <div class="elementor-container elementor-column-gap-no">
        <div
          class="elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-37efb47a footer-one"
          data-id="37efb47a"
          data-element_type="column"
        >
          <div class="elementor-widget-wrap elementor-element-populated">
            <div
              class="elementor-element elementor-element-1a7cc8e5 elementor-hidden-tablet elementor-hidden-mobile elementor-widget elementor-widget-footer-shape"
              data-id="1a7cc8e5"
              data-element_type="widget"
              data-widget_type="footer-shape.default"
            >
              <div class="elementor-widget-container">
                <div
                  class="footer-one__bg"
                  style="background-image: url(/wp-content/uploads/2023/07/footer-bg.jpg);"
                >
                </div>
                <div class="shape1 float-bob-x">
                  <img
                    decoding="async"
                    src="/wp-content/uploads/2023/07/footer-v1-shape1.png"
                    alt="alt"
                    title="footer-v1-shape1"
                  />
                </div>
                <div class="shape2 float-bob-y">
                  <img
                    decoding="async"
                    src="/wp-content/uploads/2023/07/footer-v1-shape2.png"
                    alt="alt"
                    title="footer-v1-shape2"
                  />
                </div>
                <div class="shape3 float-bob-y">
                  <img
                    decoding="async"
                    src="/wp-content/uploads/2023/07/footer-v1-shape3.png"
                    alt="alt"
                    title="footer-v1-shape3"
                  />
                </div>
                <div class="shape4 float-bob-x">
                  <img
                    decoding="async"
                    src="/wp-content/uploads/2023/07/footer-v1-shape4.png"
                    alt="alt"
                    title="footer-v1-shape4"
                  />
                </div>
              </div>
            </div>
            <section
              class="elementor-section elementor-inner-section elementor-element elementor-element-681d0305 footer-one__top elementor-section-boxed elementor-section-height-default elementor-section-height-default"
              data-id="681d0305"
              data-element_type="section"
            >
              <div class="elementor-container elementor-column-gap-default">
                <div
                  class="elementor-column elementor-col-25 elementor-inner-column elementor-element elementor-element-24995604"
                  data-id="24995604"
                  data-element_type="column"
                >
                  <div
                    class="elementor-widget-wrap elementor-element-populated"
                  >
                    <div
                      class="elementor-element elementor-element-70ddcf5a elementor-widget__width-initial elementor-widget elementor-widget-footer-about"
                      data-id="70ddcf5a"
                      data-element_type="widget"
                      data-widget_type="footer-about.default"
                    >
                      <div class="elementor-widget-container">
                        <div class="footer-one__single footer-one__about">
                          <div class="footer-one__about-logo">
                            <a href="">
                              <img
                                decoding="async"
                                loading="lazy"
                                width="179"
                                height="42"
                                src="/wp-content/uploads/2023/07/cropped-Thunder-Brain-Logo-01-1.png"
                                alt="ThunderBrains"
                              />
                            </a>
                          </div>

                          <div class="footer-one__about-text">
                            <p>
                              Times Square, #405, 196-A,
                              Phase 8B, 
                              <br />Sector 74, Sahibzada Ajit Singh Nagar,
                              Punjab 160055
                            </p>
                          </div>
                          <div class="footer-one__about-cta">
                            <p>
                              <span
                                aria-hidden="true"
                                class="ontech-icons-two-telephone"></span>
                              <a href="tel:9779308242">+91 977 93 08 242</a>
                            </p>

                            <p>
                              <span
                                aria-hidden="true"
                                class="ontech-icons-two-mail"></span>
                              <a href="mailto:contactus@thunderbrains.in"
                                >contactus@thunderbrains.in</a
                              >
                            </p>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div
                  class="elementor-column elementor-col-25 elementor-inner-column elementor-element elementor-element-5ccd46c6"
                  data-id="5ccd46c6"
                  data-element_type="column"
                >
