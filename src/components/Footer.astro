                            <div class="icon-box">
                              <span aria-hidden="true" class="icon-email2"
                              ></span>
                            </div>

                            <div class="content-box">
                              <p>Quick Email us</p>
                              <h3>
                                <a href="mailto:contactus@thunderbrains.in"
                                  >contactus@thunderbrains.in</a
                                >
                              </h3>
                            </div>
                          </div>
