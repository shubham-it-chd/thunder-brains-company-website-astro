const {} = Astro.props;

sendEmail().catch(console.error)
---
<style>
    .page-header__bg {
    background-image: url(wp-content/uploads/2023/07/page-header-bg.jpg) !important;
}
</style>
<Layout title="ContactUs"  description="Welcome to Thunder Brains Company â€“ where innovation meets brilliance." keywords="Innovation,Core Values,Our Mission,Our Services,Our Expert Team">
  
    <!--Start Page Header-->
